    <analyzer type="query">
      <tokenizer name="whitespace"/>
      <filter synonyms="synonyms.txt" name="synonymGraph" expand="false" ignoreCase="true"/>
      <filter name="stop" ignoreCase="true" words="lang/stopwords_en.txt"/>
