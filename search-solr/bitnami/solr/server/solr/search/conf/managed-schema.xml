      <filter name="stop" ignoreCase="true" words="stopwords.txt"/>
      <filter name="lowercase"/>
      <filter withOriginal="true" maxPosAsterisk="3" maxFractionAsterisk="0.33" name="reversedWildcard" maxPosQuestion="2"/>
